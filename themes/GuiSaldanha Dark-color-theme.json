            }
        },
        {
            "scope": "meta",
            "settings": {
                "foreground": "#c3bd63"
            }
        },
        {
            "scope": "meta.paragraph",
            "settings": {
                "foreground": "#cccccc"
            }
        },
        {
            "scope": "string.quoted.single, string.quoted.other",
            "settings": {
                "foreground": "#E5C07B"
            }
        },
        {
            "scope": "string.quoted.double",
            "settings": {
                "foreground": "#f3be5c"
            }
        },
        {
            "scope": "punctuation.definition.variable, punctuation.definition.entity, variable, variable.other.class",
