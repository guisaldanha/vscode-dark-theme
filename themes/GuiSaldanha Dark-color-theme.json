                "foreground": "#3ba9c2"
            }
        },
        {
            "scope": "variable.other.property",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "meta.function-call",
            "settings": {
                "foreground": "#58afef"
            }
        },
        {
            "scope": "meta.function-call.arguments",
            "settings": {
                "foreground": "#bc78dd"
            }
        },
        {
            "scope": "string.quoted.double variable.other",
            "settings": {
                "foreground": "#d86c23"
            }
        },
        {
            "scope": "keyword.control",
            "settings": {
                "foreground": "#98c363"
            }
        },
        {
            "scope": "keyword.control.import",
            "settings": {
                "fontStyle": "italic"
            }
        },

        {
            "scope": "constant.numeric, constant.integer",
            "settings": {
                "foreground": "#e06872"
            }
        },
        {
            "scope": "meta.item-access",
            "settings": {
                "foreground": "#6ce0b1"
            }
        },
        {
            "scope": "keyword.operator, punctuation.definition ",
            "settings": {
                "foreground": "#dee06c"
            }
        },
        {
            "scope": "constant.language",
            "settings": {
                "foreground": "#4ce875"
            }
        },
        {
            "scope": "keyword.other",
            "settings": {
                "foreground": "#e15ba7"
            }
        },
        {
            "scope": "storage.modifier, entity.name.function",
            "settings": {
                "foreground": "#a585d5"
            }
        },
        {
            "scope": "support.function",
            "settings": {
                "foreground": "#e66be0"
            }
        },
        {
            "scope": "support.type",
            "settings": {
                "foreground": "#56B6C2"
            }
        },
        {
            "scope": "storage.type",
            "settings": {
                "foreground": "#c3cb71"
            }
        },
        {
            "scope": "storage.type.class",
            "settings": {
                "foreground": "#e8f188"
            }
        },
        {
            "scope": "meta.class.body storage.type",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "punctuation.definition.heading, markup.heading.markdown",
            "settings": {
                "foreground": "#007fd4"
            }
        },
        {
            "scope": "markup.bold",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "markup.list meta.paragraph",
            "settings": {
                "foreground": "#c48347"
            }
        },
        {
            "scope": "markup.italic",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "markup.inline.raw.string",
            "settings": {
                "foreground": "#56B6C2"
            }
        },
        {
            "scope": "meta.embedded.block, fenced_code.block.language",
            "settings": {
                "foreground": "#56B6C2"
            }
        },
        {
            "scope": "fenced_code.block.language",
            "settings": {
                "fontStyle": "italic"
            }
        },
        {
            "scope": "entity.name.section.group-title",
            "settings": {
                "fontStyle": "bold"
            }
        },
        {
            "scope": "meta.embedded.block, meta.embedded.line",
            "settings": {
                "foreground": "#ff7171",
            }
        },
        {
            "scope": "keyword.other.phpdoc, comment.block.documentation.phpdoc.php meta.other.type.phpdoc.php keyword.other.type.php",
            "settings": {
                "foreground": "#7f848e",
            }
        },
        {
            "scope": "keyword.other.phpdoc",
            "settings": {
              "fontStyle": "bold"
            }
        },
        {
            "scope": "comment.block.documentation.phpdoc.php meta.other.type.phpdoc.php keyword.other.type.php",
            "settings": {
              "fontStyle": "italic"
            }
        },
        
    ],
