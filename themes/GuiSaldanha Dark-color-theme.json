{
    "name": "GuiSaldanha Dark",
    "type": "dark",
    "semanticHighlighting": true,
    "semanticTokenColors": {
        "enumMember": {
            "foreground": "#e06c75"
        },
        "variable.constant": {
            "foreground": "#e06872"
        },
        "variable.defaultLibrary": {
            "foreground": "#e5c07b"
        }
    },
    "tokenColors": [
        {
            "scope": "comment, punctuation.definition.comment",
            "settings": {
                "foreground": "#7f848e"
            }
        },
        {
            "scope": "keyword",
            "settings": {
                "foreground": "#98c363"
            }
        },
        {
            "scope": "entity, punctuation.definition.tag",
            "settings": {
                "foreground": "#639bc3"
            }
